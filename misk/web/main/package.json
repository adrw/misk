{
  "name": "misk-admin",
  "version": "0.0.1",
  "description": "Microservice Kontainer Modular Dashboard",
  "main": "index.js",
  "scripts": {
    "build:main": "webpack --production --config webpack.production.config.js",
    "build:lib": "webpack --production --config ../@misk/common/webpack.production.config.js; mkdir -p ./build/js/; cp ../@misk/common/lib/* ./build/js/",
    "build:modules": "for d in ../modules/*/; do webpack --production --config $d/webpack.production.config.js; done; cp ./build/js/t_* ./src/core/components/",
    "build": "npm run build:lib; npm run build:modules; npm run build:main",
    "start:main": "webpack --config webpack.development.config.js",
    "start:lib": "webpack --config ../@misk/common/webpack.development.config.js",
    "start:modules": "for d in ../modules/*/; do webpack --config $d/webpack.development.config.js; done; cp ./build/js/t_* ./src/core/components/",
    "start:all": "npm run build:lib; npm run start:lib && npm run start:modules; npm run start:main",
    "clean": "rm -rf ./build; touch ./src/core/components/t_.tmp && rm ./src/core/components/t_*",
    "rebuild": "npm run clean && npm run build",
    "dist": "npm run rebuild && webpack-dev-server --production --config ./webpack.production.config.js",
    "restart": "npm run clean && npm run start:all",
    "serve-only": "live-server build",
    "start-old": "npm run build && npm run serve-only",
    "start-only": "webpack-dev-server --config ./webpack.development.config.js",
    "start": "npm run restart && webpack-dev-server --config ./webpack.development.config.js",
    "reinstall:main": "rm -rf node_modules; yarn install",
    "reinstall:lib": "cd ../@misk/common; rm -rf node_modules; yarn install",
    "reinstall:modules": "for d in ../modules/*/; do bash -c \"cd $d; rm -rf node_modules; yarn install\"; done",
    "reinstall": "npm run reinstall:main && npm run reinstall:lib && npm run reinstall:modules"
  },
  "author": "Square/Misk Authors (https://github.com/square/misk/graphs/contributors)",
  "dependencies": {
    "@blueprintjs/core": "^2.3.1",
    "@types/history": "^4.6.2",
    "@types/react": "^16.3.16",
    "@types/react-dom": "^16.0.5",
    "frint": "^5.4.4",
    "frint-react": "^5.4.4",
    "frint-router": "^5.4.4",
    "frint-router-react": "^5.4.4",
    "frint-store": "^5.4.4",
    "lodash": "^4.17.10",
    "prop-types": "^15.6.1",
    "react": "^16.4.0",
    "react-dom": "^16.4.0",
    "react-transition-group": "^2.3.1",
    "rxjs": "^6.2.0"
  },
  "devDependencies": {
    "@types/node": "^10.3.0",
    "awesome-typescript-loader": "^5.0.0",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-eslint": "^8.2.3",
    "babel-loader": "^7.1.4",
    "babel-preset-travix": "^1.2.0",
    "babel-register": "^6.26.0",
    "css-loader": "^0.28.11",
    "file-loader": "^1.1.11",
    "frint-cli": "^5.4.4",
    "html-webpack-plugin": "^3.2.0",
    "live-server": "^1.2.0",
    "node-sass": "^4.9.0",
    "sass-loader": "^7.0.1",
    "script-ext-html-webpack-plugin": "^2.0.1",
    "source-map-loader": "^0.2.3",
    "style-loader": "^0.21.0",
    "typescript": "^2.9.1",
    "webpack": "^4.10.2",
    "webpack-cli": "^2.1.5",
    "webpack-dev-server": "^3.1.4"
  }
}
